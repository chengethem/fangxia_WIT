{
  "extends": [
    "airbnb"
  ],
  "plugins": [],
  "rules": {
    "no-console": 0,
    "camelcase": [
      0,
      {
        "properties": "never"
      }
    ],
    "comma-dangle": 0,
    "consistent-return": 0,
    "eqeqeq": 1,
    "max-len": 0,
    "no-param-reassign": 0,
    "no-shadow": 1,
    "no-var": 1,
    "no-unused-vars": [
      1,
      {
        "vars": "local",
        "args": "after-used"
      }
    ],
    "no-undef": [
      0,
      {
        "typeof": true
      }
    ],
    "prefer-rest-params": 0,
    "spaced-comment": [
      0
    ],
    "strict": [
      0,
      "global"
    ],
    "vars-on-top": 0,
    "react/prop-types": 1,
    "no-use-before-define": [
      0,
      {
        "functions": true,
        "classes": true
      }
    ],
    "new-cap": 1,
    "object-curly-spacing": 0
  }
}
